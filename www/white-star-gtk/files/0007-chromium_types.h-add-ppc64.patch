From 58d3e50c62c6427c8365a7c8d162dc3e9f3ec01f Mon Sep 17 00:00:00 2001
From: Sergey Fedorov <vital.had@gmail.com>
Date: Sun, 21 Jan 2024 06:02:39 +0800
Subject: [PATCH 7/7] chromium_types.h: add ppc64

---
 gfx/ycbcr/chromium_types.h            | 4 ++++
 ipc/chromium/src/build/build_config.h | 2 +-
 2 files changed, 5 insertions(+), 1 deletion(-)

diff --git gfx/ycbcr/chromium_types.h gfx/ycbcr/chromium_types.h
index dceac4766e..76f786c8e7 100644
--- gfx/ycbcr/chromium_types.h
+++ gfx/ycbcr/chromium_types.h
@@ -27,6 +27,10 @@
 #define ARCH_CPU_ARM_FAMILY 1
 #define ARCH_CPU_ARMEL 1
 #define ARCH_CPU_32_BITS 1
+#elif defined(__ppc64__) || defined(__powerpc64__)
+#define ARCH_CPU_PPC_FAMILY 1
+#define ARCH_CPU_PPC64 1
+#define ARCH_CPU_64_BITS 1
 #elif defined(__ppc__) || defined(__powerpc) || defined(__PPC__)
 #define ARCH_CPU_PPC_FAMILY 1
 #define ARCH_CPU_PPC 1
diff --git ipc/chromium/src/build/build_config.h ipc/chromium/src/build/build_config.h
index 5f967ebe11..a7e59ac99f 100644
--- ipc/chromium/src/build/build_config.h
+++ ipc/chromium/src/build/build_config.h
@@ -78,7 +78,7 @@
 #define ARCH_CPU_ARMEL 1
 #define ARCH_CPU_32_BITS 1
 #define WCHAR_T_IS_UNSIGNED 1
-#elif defined(__powerpc64__)
+#elif defined(__ppc64__) || defined(__powerpc64__)
 #define ARCH_CPU_PPC64 1
 #define ARCH_CPU_64_BITS 1
 #elif defined(__ppc__) || defined(__powerpc__)
-- 
