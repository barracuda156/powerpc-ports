Subject: [PATCH 6/7] SkPreConfig.h: add forgotten ppc64

---
 gfx/skia/skia/include/core/SkPreConfig.h | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git gfx/skia/skia/include/core/SkPreConfig.h gfx/skia/skia/include/core/SkPreConfig.h
index cb7eb3f3e6..2e923d7932 100644
--- gfx/skia/skia/include/core/SkPreConfig.h
+++ gfx/skia/skia/include/core/SkPreConfig.h
@@ -78,10 +78,10 @@
         #define SK_CPU_LENDIAN
     #elif defined(__sparc) || defined(__sparc__) || \
       defined(_POWER) || defined(__powerpc__) || \
-      defined(__ppc__) || defined(__hppa) || \
+      defined(__ppc__) || defined(__ppc64__) || \
       defined(__PPC__) || defined(__PPC64__) || \
       defined(_MIPSEB) || defined(__ARMEB__) || \
-      defined(__s390__) || \
+      defined(__s390__) || defined(__hppa) || \
       (defined(__sh__) && defined(__BIG_ENDIAN__)) || \
       (defined(__ia64) && defined(__BIG_ENDIAN__))
         #define SK_CPU_BENDIAN
